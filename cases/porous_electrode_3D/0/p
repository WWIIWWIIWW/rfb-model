FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    // #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        //type            fixedFluxPressure;
        //value           $internalField;
        type              zeroGradient;
    }

    // Electrode inlet, no flow
    e_in_wall
    {
        type            fixedFluxPressure;
    }

    outlet
    {
        type            fixedValue;
        value           $internalField;
    }

    // Electrode outlet, no flow
    e_out_wall
    {
        type            fixedFluxPressure;
    }


    top
    {
        type            fixedFluxPressure;
    }

    bottom
    {
        type            fixedFluxPressure;
    }

    front
    {
        type            fixedFluxPressure;
    }

    back
    {
        type            fixedFluxPressure;
    }

}

